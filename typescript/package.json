{
  "name": "nodets",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "tsc": "tsc",
    "oop:dev": "ts-node-dev --respawn   ./oop/app.ts",
    "expressjs:dev": "ts-node-dev --respawn  ./expressjs/app.ts",
    "decorators:dev": "tsc ./decorators/class-decorator.ts -w --experimentalDecorators",
    "generics:dev": "ts-node-dev --respawn  ./generics/app.ts",
    "oop:prod": "tsc && node ./build/oop/app.js",
    "expressjs:prod": "tsc && node ./build/expressjs/app.js",
    "decorators:prod": "tsc && node ./build/decorators/app.js",
    "generics:prod": "tsc && node ./build/generics/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.7",
    "express": "^4.17.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.56"
  }
}
